                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/card_padding"
                android:layout_toEndOf="@id/frameLayout1"
                android:text="@string/title_text2" />

            <TextView
                android:id="@+id/my_text_view1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/title_text_view1"
                android:layout_marginLeft="@dimen/card_padding"
                android:layout_toEndOf="@id/frameLayout1"
                android:text="@string/detail_text2" />

            <Button
                android:id="@+id/button1"
                android:layout_width="173dp"
                android:layout_height="40dp"
                android:layout_below="@id/my_text_view1"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="@dimen/card_padding"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@id/frameLayout1"
                android:background="@drawable/mystyle"
                android:text="More Info" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

